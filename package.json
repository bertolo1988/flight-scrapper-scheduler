    "test": "npm run lint && mocha test --recursive",
    "debug": "npm run lint && set DEBUG=flight-scrapper-scheduler && npm start",
    "full-debug": "npm run lint && set DEBUG=* && npm start",
    "start": "node scheduler-demo.js"
  },
  "dependencies": {
    "cron": "1.1.0",
    "debug": "2.2.0",
    "flight-scrapper": "0.0.8",
    "moment": "2.14.1"
