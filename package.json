  "main": "dist/flight-scrapper-scheduler.js",
  "scripts": {
    "mongo-mac": "mkdir mongo-db; mongod --dbpath mongo-db",
    "mongo-win": "mkdir mongo-db & \"C:/Program Files/MongoDb/Server/3.2/bin/mongod.exe\" --dbpath=mongo-db",
